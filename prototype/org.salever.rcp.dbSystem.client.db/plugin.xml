<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.salever.rcp.dbSystem.client.db.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.salever.rcp.dbSystem.client.db.category"
            class="org.salever.rcp.dbSystem.client.db.preferences.MySQLPreferencePage"
            id="org.salever.rcp.dbSystem.client.db.preferences.DBPreferencePage"
            name="%page.name">
      </page>
      <page
            class="org.salever.rcp.dbSystem.client.db.preferences.DBCategoryPage"
            id="org.salever.rcp.dbSystem.client.db.category"
            name="%page.name.DB">
      </page>
   </extension>

</plugin>
