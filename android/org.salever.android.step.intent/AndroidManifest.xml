<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.salever.android.step.intent" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".MainActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name="CustomBroadcastReceiver1">
			<intent-filter>
				<action android:name="org.salever.android.step.intent.action.broadcast1"></action>
			</intent-filter>

</receiver>
		<receiver android:name="CustomBroadcastReceiver2">
			<intent-filter>
				<action android:name="org.salever.android.step.intent.action.broadcast2"></action>
			</intent-filter>
		</receiver>
	</application>


</manifest> 